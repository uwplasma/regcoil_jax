! Port of regcoil/examples/regcoilPaper_figure3_NF4 for regcoil_jax.
!
! This case uses a different equilibrium (li383, NFP=4) and a realistic winding surface from NESCOIL.
! It is *slow* (128x128 grids) and is not included in CI.

&regcoil_nml
  general_option=1

  nlambda=1
  lambda_min = 1e-50
  lambda_max = 1

  ntheta_plasma=128
  ntheta_coil  =128
  nzeta_plasma =128
  nzeta_coil   =128

  mpol_potential = 4
  ntor_potential = 4

  geometry_option_plasma = 2
  wout_filename = 'wout_li383_1.4m.nc'

  geometry_option_coil=3
  nescin_filename = 'nescin.li383_realWindingSurface'

  net_poloidal_current_Amperes = 1.0
  net_toroidal_current_Amperes = 0.0

  load_bnorm=.t.
  bnorm_filename='bnorm.li383_1.4m'

  symmetry_option = 1
/

