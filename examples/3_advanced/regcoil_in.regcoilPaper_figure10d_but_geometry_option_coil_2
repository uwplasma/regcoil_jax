! Port of regcoil/examples/regcoilPaper_figure10d_but_geometry_option_coil_2 for regcoil_jax.
!
! High-resolution version of the Figure 10d-style run (VMEC + BNORM + uniform offset surface).
! This run is *slow* and is not included in CI.

&regcoil_nml
  nlambda = 10
  lambda_min = 1e-15
  lambda_max = 1e-14

  ntheta_plasma = 128
  ntheta_coil   = 128
  nzeta_plasma  = 128
  nzeta_coil    = 128
  mpol_potential = 32
  ntor_potential = 32

  geometry_option_plasma = 2
  wout_filename = 'wout_d23p4_tm.nc'

  geometry_option_coil=2
  separation = 0.5

  load_bnorm=.t.
  bnorm_filename='bnorm.d23p4_tm'

  net_poloidal_current_Amperes = 1.0
  net_toroidal_current_Amperes = 0.0

  symmetry_option = 1
/

